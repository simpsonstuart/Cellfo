                    <TextBlock HorizontalAlignment="Left" Margin="41,375,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="336" Height="72" FontSize="17">
            			<Run Text="-Recommended "/>
            			<Run Text="for Smartphone's"/>
            			<LineBreak/>
