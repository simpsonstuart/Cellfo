                    </TextBlock>
                    <TextBlock x:Name="Data_Allotment1" HorizontalAlignment="Left" Margin="41,311,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="17" Width="336" Height="32" Text="5GB full speed per month"/>
                    <TextBlock HorizontalAlignment="Left" Margin="41,375,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="336" Height="72" FontSize="17">
            			<Run Text="-Recommended "/>
            			<Run Text="for Smartphone's"/>
            			<LineBreak/>
            			<Run Text="-$15 Activation Fee"/>
