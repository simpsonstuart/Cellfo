                    	<Run Text="-Recommended "/>
                    	<Run Text="for Smartphone's"/>
                    	<LineBreak/>
                    	<Run Text="-$15 Activation Fee"/>
                    </TextBlock>
                </Grid>
            </phone:PanoramaItem>
            <phone:PanoramaItem CacheMode="{x:Null}" Header="$60 Smart">
                <phone:PanoramaItem.Background>
                    <SolidColorBrush Color="#FF211F1F" Opacity="0.5"/>
                </phone:PanoramaItem.Background>
                <phone:PanoramaItem.RenderTransform>
